<beast version="1.10.4">
	

	<!-- The list of taxa to be analysed (can also include dates/ages).          -->
	<!-- ntax=8                                                                  -->
	<taxa id="taxa">
		<taxon id="human"/>
		<taxon id="chimpanzee"/>
		<taxon id="gorilla"/>
		<taxon id="orangutan"/>
		<taxon id="macaca"/>
		<taxon id="howler_monkey"/>
		<taxon id="bushbaby"/>
		<taxon id="flying_lemur"/>
	</taxa>
	
	

	<!-- The sequence alignment (each sequence refers to a taxon above).         -->
	<!-- ntax=8 nchar=3369                                                       -->
	<alignment id="alignment" dataType="nucleotide">
		<sequence>
			<taxon idref="human"/>
			
		</sequence>
		<sequence>
			<taxon idref="chimpanzee"/>
			
		</sequence>
		<sequence>
			<taxon idref="gorilla"/>
			
		</sequence>
		<sequence>
			<taxon idref="orangutan"/>
			
		</sequence>
		<sequence>
			<taxon idref="macaca"/>
			
		</sequence>
		<sequence>
			<taxon idref="howler_monkey"/>
			
		</sequence>
		<sequence>
			<taxon idref="bushbaby"/>
			
		</sequence>
		<sequence>
			<taxon idref="flying_lemur"/>
			
		</sequence>
	</alignment>
	
	

	<convert id="codonAlignment" dataType="codon" geneticCode="universal">
        <alignment idref="alignment"/>
    </convert>


	<!-- The unique patterns from 1 to end                                       -->
	<!-- npatterns=816                                                           -->
	<patterns id="patterns" from="1" strip="false">
		<alignment idref="codonAlignment"/>
	</patterns>



	<newick id="startingTree">
	(((((((human:0.01, chimpanzee:0.01):0.01, gorilla:0.02):0.01, orangutan:0.03):0.01, macaca:0.04):0.01, howler_monkey:0.05):0.01, bushbaby:0.06):0.01, flying_lemur:0.07);
	</newick>

	<!-- Generate a tree model                                                   -->
	<treeModel id="treeModel">
		<coalescentTree idref="startingTree"/>
		<rootHeight>
			<parameter id="treeModel.rootHeight"/>
		</rootHeight>
		<nodeHeights internalNodes="true">
			<parameter id="treeModel.internalNodeHeights"/>
		</nodeHeights>
		<nodeHeights internalNodes="true" rootNode="true">
			<parameter id="treeModel.allInternalNodeHeights"/>
		</nodeHeights>
	</treeModel>


	<!-- Statistic for sum of the branch lengths of the tree (tree length)       -->
	<treeLengthStatistic id="treeLength">
		<treeModel idref="treeModel"/>
	</treeLengthStatistic>

	<!-- Statistic for time of most recent common ancestor of tree               -->
	<tmrcaStatistic id="age(root)" absolute="true">
		<treeModel idref="treeModel"/>
	</tmrcaStatistic>

    <!-- Frequence Model -->
	<frequencyModel id="freqModel" dataType="codon-universal" codonFromNucleotide="true">
		<alignment idref="alignment"/>
		<!-- <alignment idref="codonAlignment"/> -->
		<frequencies>
			<parameter id="nucleotidefrequencies" dimension="4"/>
		</frequencies>
     </frequencyModel>

   <!-- Codon Model -->
    <yangCodonModel id="yg" geneticCode="universal">
        <omega>
            <parameter id="omega" value="0.1" lower="0"/>
        </omega>
        <kappa>
            <parameter id="kappa" value="1.5" lower="0"/>
        </kappa>
        <frequencyModel idref="freqModel"/>
    </yangCodonModel> 

	<arbitraryBranchRates id="sbranchSpecific.omegas" centerAtOne="false" randomizeRates="false">
		<treeModel idref="treeModel"/>
		<rates>
			<parameter id="omega.rates" value="1.0" lower="0"/>
        </rates>
	</arbitraryBranchRates>

	<bayesianBridgeDistribution id="bbDistribution">
		<slabWidth>
			<parameter id="slabWidth" value="2.0"/>
		</slabWidth>
	 	<globalScale>
	 		<parameter id="globalScale" value="1" lower="0"/>
	 	</globalScale>
 		<exponent>
 			<parameter id="exponent" value="0.9"/>
 		</exponent>
		<localScale>
			<parameter id="localScale" value="1" lower="0.0" dimension="14"/>
		</localScale>
	</bayesianBridgeDistribution>

	<autoCorrelatedRatesPrior id="substBranchRatesPrior.omegas" scaling="none" log="true" operateOnIncrements="true">
		<arbitraryBranchRates idref="sbranchSpecific.omegas"/>
 		<bayesianBridgeDistribution idref="bbDistribution"/>
 	</autoCorrelatedRatesPrior>

	<fixedEffects id="fixedEffectsModel.omegas" timeEffect="false">
		<treeModel idref="treeModel"/>
		<parameter id="omega.location" lower="0.0" value="1E-1"/>
<!-- 		<signTransform/> -->
	</fixedEffects>
	
	<locationScaledBranchRateModel id="scaled.omegas">
		<treeModel idref="treeModel"/>
		<arbitraryBranchRates idref="sbranchSpecific.omegas"/>
		<fixedEffects idref="fixedEffectsModel.omegas"/>
	</locationScaledBranchRateModel>

    <branchSpecificSubstitutionParameterBranchModel id="branchSpecificParameters">
    	<substitutionModel idref="yg"/>
    	<treeModel idref="treeModel"/>
    	<substitutionParameters>
	    	<parameter idref="omega"/>
    	</substitutionParameters>
    	<branchRateModels>
    		<locationScaledBranchRateModel idref="scaled.omegas"/>
    	</branchRateModels>
    </branchSpecificSubstitutionParameterBranchModel>

    <siteModel id="siteModel">
        <substitutionModel>
            <yangCodonModel idref="yg"/>
        </substitutionModel>
        <gammaShape gammaCategories="4">
            <parameter id="siteModel.alpha" value="1.25" lower="0.0"/>
        </gammaShape>
    </siteModel>

    <arbitraryBranchRates id="branchRates" reciprocal="false" exp="false" centerAtOne="false">
        <rates>
            <parameter id="branchRates.rates" value="1" lower="0"/>
        </rates>
        <treeModel idref="treeModel"/>
    </arbitraryBranchRates>


	<!-- Likelihood for tree given sequence data                                 -->
	<treeDataLikelihood id="treeLikelihood" useAmbiguities="true" usePreOrder="true"  scalingScheme="dynamic" delayScaling="true" branchRateDerivative="true"
	branchInfinitesimalDerivative="true">
		<patterns idref="patterns"/>
		<siteModel idref="siteModel"/>
		<treeModel idref="treeModel"/>
		<arbitraryBranchRates idref="branchRates"/>
		<branchSpecificSubstitutionParameterBranchModel idref="branchSpecificParameters"/>
	</treeDataLikelihood>

	<report>
		<treeDataLikelihood idref="treeLikelihood"/>
	</report>

	<branchSubstitutionParameterGradient id="omega.gradient" traitName="SubstitutionParameterGradient.omega" useHessian="false" dim="0">
	    <treeDataLikelihood idref="treeLikelihood"/>
		<branchSpecificSubstitutionParameterBranchModel idref="branchSpecificParameters"/>
		<locationScaledBranchRateModel idref="scaled.omegas"/>
    </branchSubstitutionParameterGradient>

	<gradientWrtIncrements id="incrementGradient.omega">
		<autoCorrelatedRatesPrior idref="substBranchRatesPrior.omegas"/>
	</gradientWrtIncrements>

	<branchRateGradientWrtIncrements id="branchRateGradientWrtIncrements.omegas">
		<branchSubstitutionParameterGradient idref="omega.gradient"/>
		<gradientWrtIncrements idref="incrementGradient.omega"/>
	</branchRateGradientWrtIncrements>

	<jointGradient id="jointIncrementGradient.omega">
		<branchRateGradientWrtIncrements idref="branchRateGradientWrtIncrements.omegas"/>
		<gradientWrtIncrements idref="incrementGradient.omega"/>
	</jointGradient>	

	<nodeHeightGradient id="nodeHeightGradient" traitName="Sequence">
		<treeDataLikelihood idref="treeLikelihood"/>
	</nodeHeightGradient>

	<nodeHeightTransform id="fullRatioRealLineTransform" withRoot="true">
		<tree idref="treeModel"/>
		<arbitraryBranchRates idref="branchRates"/>
		<nodeHeights>
			<nodeHeightProxyParameter id="nodeHeightProxyParameter" rootNode="true">
				<tree idref="treeModel"/>
			</nodeHeightProxyParameter>
		</nodeHeights>
		<ratios realLine="true">
			<parameter id="nodeHeight.ratios"/>
		</ratios>
	</nodeHeightTransform>

	<operators id="operators" optimizationSchedule="log">

		<hamiltonianMonteCarloOperator weight="1" nSteps="4" stepSize="1E-3" mode="vanilla" instabilityHandler="ignore"
									   drawVariance="1.0" autoOptimize="true" targetAcceptanceProbability="0.2"
									   preconditioning="adaptiveDiagonal" preconditioningDelay="100" preconditioningUpdateFrequency="10">
			<nodeHeightGradient idref="nodeHeightGradient"/>
			<nodeHeightProxyParameter idref="nodeHeightProxyParameter"/>
			<nodeHeightTransform idref="fullRatioRealLineTransform"/>
		</hamiltonianMonteCarloOperator>

        <hamiltonianMonteCarloOperator weight="10" nSteps="4" stepSize="1E-1" mode="vanilla"
        	drawVariance="1.0" autoOptimize="true"
			preconditioning="adaptiveDiagonal" preconditioningDelay="100" preconditioningUpdateFrequency="10"
			gradientCheckCount="0" gradientCheckTolerance="0">
        	<jointGradient idref="jointIncrementGradient.omega"/>
 		</hamiltonianMonteCarloOperator>

		<scaleOperator scaleFactor="0.75" weight="4">
			 <parameter idref="omega.rates"/>
		</scaleOperator>

		<scaleOperator scaleFactor="0.75" weight="10">
			 <parameter idref="omega.location"/>
		</scaleOperator>

        <uniformOperator scaleFactor="0.75" weight="15.0">
            <parameter idref="treeModel.internalNodeHeights"/>
        </uniformOperator>

		<scaleOperator scaleFactor="0.75" weight="3">
			<parameter idref="treeModel.rootHeight"/>
		</scaleOperator>

	 	<deltaExchange delta="0.01" weight="20.0">
			<parameter idref="nucleotidefrequencies"/>
		</deltaExchange>
		<scaleOperator scaleFactor="0.75" weight="10.0">
			<parameter idref="kappa"/>
		</scaleOperator>
		<scaleOperator scaleFactor="0.75" weight="1.0">
			<parameter idref="siteModel.alpha"/>
		</scaleOperator>


	</operators>

	<!-- Define MCMC                                                             -->
	<mcmc id="mcmc" chainLength="2000000" autoOptimize="true" fullEvaluation="0">
		<posterior id="posterior">
			<prior id="prior">
			

				<dirichletPrior alpha="1.0" sumsTo="1.0">
					<parameter idref="nucleotidefrequencies"/>
				</dirichletPrior>
				<exponentialPrior mean="0.5" offset="0.0">
					<parameter idref="siteModel.alpha"/>
				</exponentialPrior>
				<exponentialPrior offset="0" mean="1">
                    <parameter idref="kappa"/>
                </exponentialPrior>
				<autoCorrelatedRatesPrior idref="substBranchRatesPrior.omegas"/>
				<exponentialPrior mean="2.0" offset="0.0">
					<parameter idref="omega.location"/>
				</exponentialPrior>

			</prior>
			<likelihood id="likelihood">
				<treeDataLikelihood idref="treeLikelihood"/>
			</likelihood>
		</posterior>
		<operators idref="operators"/>

		<!-- write log to screen                                                     -->
		<log id="screenLog" logEvery="100">
			<column label="Posterior" dp="4" width="12">
				<posterior idref="posterior"/>
			</column>


			<column label="rootHeight" sf="6" width="12">
				<parameter idref="treeModel.rootHeight"/>
			</column>

			<column label="age(root)" sf="6" width="12">
				<tmrcaStatistic idref="age(root)"/>
			</column>

		</log>

		<!-- write log to file                                                       -->
		<log id="fileLog" logEvery="1000" fileName="BRCA1_coding_shrinkage_fixedRates.log" overwrite="false">
			<posterior idref="posterior"/>
			<prior idref="prior"/>
			<likelihood idref="likelihood"/>
			<parameter idref="treeModel.rootHeight"/>
			<parameter idref="kappa"/>

			<parameter idref="siteModel.alpha"/>

			<tmrcaStatistic idref="age(root)"/>
			<treeLengthStatistic idref="treeLength"/>
			<parameter idref="nucleotidefrequencies"/>

		</log>

		
		<log id="fileLog.check" logEvery="20" fileName="BRCA1_coding_shrinkage_fixedRates.large.parameters.log" overwrite="false">
			<posterior idref="posterior"/>
			<prior idref="prior"/>
			<likelihood idref="likelihood"/>
			<parameter idref="kappa"/>


			<parameter idref="globalScale"/>
			<parameter idref="omega.location"/>
			<incrementParameter>
				<autoCorrelatedRatesPrior idref="substBranchRatesPrior.omegas"/>
			</incrementParameter>
			<parameter idref="omega.rates"/>

		</log>

		<!-- write tree log to file                                                  -->
		<logTree id="treeFileLog" logEvery="20" nexusFormat="true" fileName="BRCA1_coding_shrinkage_fixedRates.trees" sortTranslationTable="true"
			mapNamesToNumbers="false">
			<treeModel idref="treeModel"/>

			<trait name="omegas" tag="omega">
				<locationScaledBranchRateModel idref="scaled.omegas"/>
			</trait>
			<trait name="branchRates" tag="branchRates">
				<arbitraryBranchRates idref="branchRates"/>
			</trait>
			<posterior idref="posterior"/>
		</logTree>

	</mcmc>
	<report fileName="BRCA1_coding_shrinkage_fixedRates_timer.txt">
		<property name="timer">
			<object idref="mcmc"/>
		</property>
	</report>

	<report>
		<property name="timer">
			<object idref="mcmc"/>
		</property>
	</report>
</beast>
